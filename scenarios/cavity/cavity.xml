<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="500" type="turbulent"/>
    <simulation finalTime="0.10" >
    <type nulimiter="0">dns</type>
    <!--<type nulimiter="0">algebraic</type>-->
    <scenario uniform="true">cavity</scenario>
    </simulation>
    <timestep dt="1" tau="0.5" />
    <solver gamma="0.5" />
    <geometry dim="2"
      lengthX="1.0" lengthY="1.0" lengthZ="1.0" sizeX="100" sizeY="100" sizeZ="20"
    >
      <mesh>uniform</mesh>
    </geometry>
    <environment gx="0" gy="0" gz="0" />
    <walls>
        <left>
            <vector x="0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="1" y="0" z="0" />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="0.01">results/a</vtk>
    <checkpoints enabled="true">checkpoints</checkpoints>
    <stdOut interval="0.01" />
    <parallel numProcessorsX="2" numProcessorsY="2" numProcessorsZ="1" />
</configuration>
